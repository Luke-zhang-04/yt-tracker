{
    "name": "YouTube time tracker",
    "version": "1.0",
    "description": "Yes",
    "manifest_version": 2,

    "permissions": ["storage", "tabs"],
    "background": {
        "scripts": ["dist/tracker.js"],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "ui/index.html"
    }
}